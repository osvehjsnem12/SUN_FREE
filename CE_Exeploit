local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()
 
local PhantomForcesWindow = Library:NewWindow("CE 테러 허브")
 
local KillingCheats = PhantomForcesWindow:NewSection("CE 스크 모음")
 
KillingCheats:CreateButton("CE 폭발 올", function()

        
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- CE_Events 찾기
local CE_Events
for i, v in next, getgc(true) do 
    if type(v) == 'table' and rawget(v, 'damageEvent') and rawget(v, 'equipEvent') then
        CE_Events = v
        break -- CE_Events가 발견되면 루프를 종료
    end
end

-- CE_Events가 존재하는 경우 처리
if CE_Events then
    for _, plr in pairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer then
            -- explosiveEvent 호출 (폭발 이벤트)
            CE_Events["explosiveEvent"]:FireServer(nil, plr.Character:GetPivot().Position, 50000, 1213320, 1321330, nil, nil, nil, nil, nil, nil, nil, "Auth")
        end
    end
end
        
end)

KillingCheats:CreateButton("CE 핑핵1", function()

        
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- CE_Events 찾기
local CE_Events
for i, v in next, getgc(true) do 
    if type(v) == 'table' and rawget(v, 'damageEvent') and rawget(v, 'equipEvent') then
        CE_Events = v
        break -- CE_Events가 발견되면 루프 종료
    end
end

-- CE_Events가 존재하는 경우 처리
if CE_Events then
    while true do
        for _, plr in pairs(Players:GetPlayers()) do
            if plr ~= LocalPlayer and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
                -- 폭발 이벤트 발생 (데미지 이벤트 제외)
                CE_Events["explosiveEvent"]:FireServer(nil, plr.Character.HumanoidRootPart.Position, 50000, 1213320, 1321330, nil, nil, nil, nil, nil, nil, nil, "Auth")
                print(plr.Name .. "에게 폭발 이벤트를 발생시켰습니다.")
            end
        end
        wait(0.1) -- 0.1초 간격으로 반복
    end
end
        
end)

KillingCheats:CreateButton("CE 핑핵2", function()

        
loadstring(game:HttpGet("https://raw.githubusercontent.com/Dain29292929/Ce1/refs/heads/main/Ce1"))
        
end)

KillingCheats:CreateButton("CE 무적", function()


local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- CE_Events 찾기
local CE_Events
for i, v in next, getgc(true) do
    if type(v) == 'table' and rawget(v, 'damageEvent') and rawget(v, 'explosiveEvent') then
        CE_Events = v
        break
    end
end

-- 무적 적용 함수
local function makeInvincible()
    if LocalPlayer.Character then
        local humanoid = LocalPlayer.Character:FindFirstChild("Humanoid")
        local rootPart = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")

        if humanoid then
            humanoid.MaxHealth = math.huge
            humanoid.Health = math.huge
            humanoid.BreakJointsOnDeath = false -- 죽어도 캐릭터가 부서지지 않도록 설정
        end

        if rootPart then
            -- 충격 저항을 조절하여 폭발로 밀려나는 것을 방지
            rootPart.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0, 0, 0)
        end
    end
end

-- CE_Events로 로컬 플레이어 대미지 & 폭발 무효화
if CE_Events then
    local oldDamageEvent = CE_Events["damageEvent"]
    local oldExplosiveEvent = CE_Events["explosiveEvent"]

    -- 대미지 무효화
    CE_Events["damageEvent"] = function(target, damage)
        if target == LocalPlayer.Character:FindFirstChild("Humanoid") then
            return -- 로컬 플레이어에게 들어오는 대미지를 무효화
        else
            return oldDamageEvent(target, damage) -- 다른 플레이어는 정상적으로 대미지 적용
        end
    end

    -- 폭발 무효화
    CE_Events["explosiveEvent"] = function(...)
        print("폭발 이벤트 무효화됨")
        return -- 폭발 이벤트 차단
    end
end

-- 무적 적용
makeInvincible()
LocalPlayer.CharacterAdded:Connect(makeInvincible) -- 캐릭터 리스폰 시에도 무적 유지

print("로컬 플레이어에게 완전한 무적이 적용되었습니다.")
        
end)

KillingCheats:CreateButton("CE 브링 올", function()

        
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local CE_Events

-- CE_Events 찾기
for i, v in next, getgc(true) do
    if type(v) == "table" and rawget(v, "giveEvent") then
        CE_Events = v
        break
    end
end

-- 로컬 플레이어의 위치로 3 스터디 범위 내로 다른 플레이어 이동시키기
local function movePlayersToLocalPlayer()
    local localPosition = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Position

    if localPosition then
        for _, plr in pairs(Players:GetPlayers()) do
            -- 로컬 플레이어는 제외하고, 다른 플레이어들을 이동시킴
            if plr ~= LocalPlayer and plr.Character then
                local plrHumanoidRoot = plr.Character:FindFirstChild("HumanoidRootPart")
                if plrHumanoidRoot then
                    -- 3 스터디 (3 미터) 거리만큼 이동시키기
                    local newPosition = localPosition + Vector3.new(math.random(-3, 3), 0, math.random(-3, 3))
                    plrHumanoidRoot.CFrame = CFrame.new(newPosition) -- 새로운 위치로 이동
                end
            end
        end
    end
end

-- 모든 플레이어를 로컬 플레이어의 3 스터디 범위 내로 이동시키기
movePlayersToLocalPlayer()

print("모든 플레이어가 로컬 플레이어의 3 스터디 범위 내로 이동되었습니다.")
        
end)

KillingCheats:CreateButton("CE 맵 삭제", function()

        
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

-- CE_Events 찾기
local CE_Events
for i, v in next, getgc(true) do
    if type(v) == 'table' and rawget(v, 'damageEvent') and rawget(v, 'equipEvent') then
        CE_Events = v
        break
    end
end

-- 맵의 모든 파트 삭제 함수
local function deleteMapObjects()
    for _, object in pairs(workspace:GetChildren()) do
        if object:IsA("Part") or object:IsA("Model") then
            -- 파트나 모델 삭제
            object:Destroy()
        end
    end
end

-- CE_Events로 로컬 플레이어 대미지 & 폭발 무효화
if CE_Events then
    -- 모든 플레이어에게 맵 삭제 명령
    for _, plr in pairs(Players:GetPlayers()) do
        if plr ~= Players.LocalPlayer then  -- 로컬 플레이어 제외
            CE_Events["damageEvent"]:FireServer(nil, 0)  -- 대미지 이벤트 예시
            -- 필요에 따라 다른 이벤트를 추가로 처리 가능
        end
    end
end

-- 맵을 삭제하고 싶을 때 실행
deleteMapObjects()

print("모든 맵 오브젝트가 삭제되었습니다.")
        
end)

KillingCheats:CreateButton("CE 모든 플레이어에게 공지 띄어기", function()

        
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

-- CE_Events 찾기
local CE_Events
for i, v in next, getgc(true) do
    if type(v) == 'table' and rawget(v, 'damageEvent') and rawget(v, 'equipEvent') then
        CE_Events = v
        break
    end
end

-- 허브 UI 생성 함수
local function createHub(player)
    -- 허브 프레임 생성
    local screenGui = Instance.new("ScreenGui")
    screenGui.Parent = player.PlayerGui
    screenGui.IgnoreGuiInset = true

    local hubFrame = Instance.new("Frame")
    hubFrame.Size = UDim2.new(0.5, 0, 0.3, 0)  -- 허브 크기 설정
    hubFrame.Position = UDim2.new(0.25, 0, 0.35, 0)  -- 화면 중앙에 배치
    hubFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    hubFrame.BackgroundTransparency = 0.5  -- 반투명
    hubFrame.Parent = screenGui

    -- 텍스트 라벨 생성
    local textLabel = Instance.new("TextLabel")
    textLabel.Size = UDim2.new(1, 0, 1, 0)
    textLabel.Position = UDim2.new(0, 0, 0, 0)
    textLabel.Text = "https://discord.gg/KFqPnmGBTZ"
    textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)  -- 흰색 텍스트
    textLabel.TextSize = 24
    textLabel.TextCentered = true
    textLabel.TextWrapped = true
    textLabel.Parent = hubFrame

    -- 카운트다운 텍스트
    local countDownLabel = Instance.new("TextLabel")
    countDownLabel.Size = UDim2.new(1, 0, 0.2, 0)
    countDownLabel.Position = UDim2.new(0, 0, 0.8, 0)
    countDownLabel.Text = "3"
    countDownLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
    countDownLabel.TextSize = 30
    countDownLabel.TextCentered = true
    countDownLabel.Parent = hubFrame

    -- 3초 카운트 및 삭제
    local count = 3
    spawn(function()
        for i = 1, count do
            countDownLabel.Text = tostring(count - i + 1)
            wait(1)
        end
        screenGui:Destroy()  -- 3초 후 허브 삭제
    end)
end

-- 모든 플레이어에게 허브 띄우기
for _, plr in pairs(Players:GetPlayers()) do
    if plr ~= Players.LocalPlayer then  -- 로컬 플레이어 제외
        createHub(plr)
    end
end

print("모든 플레이어에게 허브를 띄웠습니다.")
        
end)
