local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()
 
local PhantomForcesWindow = Library:NewWindow("CE 테러 허브")
 
local KillingCheats = PhantomForcesWindow:NewSection("CE 스크 모음")
 
KillingCheats:CreateButton("디코: https://discord.gg/KFqPnmGBTZ", function()

        
print("https://discord.gg/KFqPnmGBTZ")
        
end)

KillingCheats:CreateButton("CE 폭발 올", function()

       
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- CE_Events 찾기
local CE_Events
for i, v in next, getgc(true) do 
    if type(v) == 'table' and rawget(v, 'damageEvent') and rawget(v, 'equipEvent') then
        CE_Events = v
        break -- CE_Events가 발견되면 루프를 종료
    end
end

-- CE_Events가 존재하는 경우 처리
if CE_Events then
    for _, plr in pairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer then
            -- explosiveEvent 호출 (폭발 이벤트)
            CE_Events["explosiveEvent"]:FireServer(nil, plr.Character:GetPivot().Position, 50000, 1213320, 1321330, nil, nil, nil, nil, nil, nil, nil, "Auth")
        end
    end
end
        
end)

KillingCheats:CreateButton("CE 핑핵", function()


local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local CE_Events

-- CE_Events 찾기
for i, v in next, getgc(true) do
    if type(v) == 'table' and rawget(v, 'damageEvent') and rawget(v, 'equipEvent') then
        CE_Events = v
        break
    end
end

-- 0.1초마다 폭발을 발생시키는 함수
local function startExplosions()
    if not CE_Events then return end
    local startTime = tick() -- 시작 시간 기록

    while tick() - startTime < 5 do  -- 5초 동안 실행
        for _, plr in pairs(Players:GetPlayers()) do
            if plr ~= LocalPlayer and plr.Character then
                local targetHRP = plr.Character:FindFirstChild("HumanoidRootPart")
                if targetHRP then
                    -- 폭발 이벤트 실행
                    CE_Events["explosiveEvent"]:FireServer(nil, targetHRP.Position, 50000, 1213320, 1321330, nil, nil, nil, nil, nil, nil, nil, "Auth")
                end
            end
        end
        wait(0.1) -- 0.1초 간격으로 실행
    end

    print("5초가 지나 폭발이 중지되었습니다.")
end

-- 폭발 시작
startExplosions()
        
end)

KillingCheats:CreateButton("CE 무적", function()


local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- CE_Events 찾기
local CE_Events
for i, v in next, getgc(true) do
    if type(v) == 'table' and rawget(v, 'damageEvent') and rawget(v, 'explosiveEvent') then
        CE_Events = v
        break
    end
end

-- 무적 적용 함수
local function makeInvincible()
    if LocalPlayer.Character then
        local humanoid = LocalPlayer.Character:FindFirstChild("Humanoid")
        local rootPart = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")

        if humanoid then
            humanoid.MaxHealth = math.huge
            humanoid.Health = math.huge
            humanoid.BreakJointsOnDeath = false -- 죽어도 캐릭터가 부서지지 않도록 설정
        end

        if rootPart then
            -- 충격 저항을 조절하여 폭발로 밀려나는 것을 방지
            rootPart.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0, 0, 0)
        end
    end
end

-- CE_Events로 로컬 플레이어 대미지 & 폭발 무효화
if CE_Events then
    local oldDamageEvent = CE_Events["damageEvent"]
    local oldExplosiveEvent = CE_Events["explosiveEvent"]

    -- 대미지 무효화
    CE_Events["damageEvent"] = function(target, damage)
        if target == LocalPlayer.Character:FindFirstChild("Humanoid") then
            return -- 로컬 플레이어에게 들어오는 대미지를 무효화
        else
            return oldDamageEvent(target, damage) -- 다른 플레이어는 정상적으로 대미지 적용
        end
    end

    -- 폭발 무효화
    CE_Events["explosiveEvent"] = function(...)
        print("폭발 이벤트 무효화됨")
        return -- 폭발 이벤트 차단
    end
end

-- 무적 적용
makeInvincible()
LocalPlayer.CharacterAdded:Connect(makeInvincible) -- 캐릭터 리스폰 시에도 무적 유지

print("로컬 플레이어에게 완전한 무적이 적용되었습니다.")
        
end)

KillingCheats:CreateButton("CE 브링올", function()

        
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- CE_Events 찾기
local CE_Events
for i, v in next, getgc(true) do
    if type(v) == 'table' and rawget(v, 'damageEvent') and rawget(v, 'equipEvent') then
        CE_Events = v
        break
    end
end

-- 로컬 플레이어 앞 3 스터드 지점으로 다른 플레이어 이동
if CE_Events and LocalPlayer.Character then
    local rootPart = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")

    if rootPart then
        local forwardVector = rootPart.CFrame.LookVector * 3  -- 정면 방향으로 3 스터드 이동
        local targetPosition = rootPart.Position + forwardVector

        for _, plr in pairs(Players:GetPlayers()) do
            if plr ~= LocalPlayer and plr.Character then
                local targetCharacter = plr.Character
                local targetHumanoidRootPart = targetCharacter:FindFirstChild("HumanoidRootPart")
                
                -- 로컬 플레이어 앞 3 스터드 위치로 이동
                if targetHumanoidRootPart then
                    targetHumanoidRootPart.CFrame = CFrame.new(targetPosition)
                end
            end
        end
    end
end

print("모든 플레이어가 로컬 플레이어의 정면 3 스터드 앞에 이동했습니다.")
        
end)

KillingCheats:CreateButton("CE 모든 총 얻기", function()

        
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local LocalPlayer = Players.LocalPlayer
local Backpack = LocalPlayer:FindFirstChild("Backpack")

local CE_Weapons = {}

-- getgc를 사용하여 CE 무기 테이블 찾기
for i, v in next, getgc(true) do
    if type(v) == "table" and rawget(v, "fireEvent") and rawget(v, "reloadEvent") then
        table.insert(CE_Weapons, v)
    end
end

-- 무기를 획득하는 함수
local function giveWeapon(weapon)
    if weapon and weapon["equipEvent"] then
        weapon["equipEvent"]:FireServer() -- 장착
        print("무기 획득: " .. tostring(weapon))
    end
end

-- 모든 CE 총 획득
for _, weapon in pairs(CE_Weapons) do
    giveWeapon(weapon)
end

print("모든 CE 총을 획득했습니다!")
        
end)

KillingCheats:CreateButton("CE 총 조작", function()

        
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local Backpack = LocalPlayer:FindFirstChild("Backpack")

local CE_Weapons = {}

-- getgc를 사용하여 CE 무기 테이블 찾기
for i, v in next, getgc(true) do
    if type(v) == "table" and rawget(v, "fireEvent") and rawget(v, "reloadEvent") then
        table.insert(CE_Weapons, v)
    end
end

-- 무기 속성 수정 함수
local function modifyWeapon(weapon)
    if weapon then
        if rawget(weapon, "fireRate") then
            weapon["fireRate"] = 0.05  -- 발사 속도를 0.05초로 변경
        end
        if rawget(weapon, "ammo") then
            weapon["ammo"] = math.huge  -- 탄창을 무한으로 설정
        end
        if rawget(weapon, "maxAmmo") then
            weapon["maxAmmo"] = math.huge  -- 최대 탄약도 무한으로 설정
        end
        print("수정 완료: " .. tostring(weapon))
    end
end

-- 로컬 플레이어가 가진 모든 CE 총 수정
for _, weapon in pairs(CE_Weapons) do
    modifyWeapon(weapon)
end

print("모든 CE 총의 발사 속도를 0.05로 변경하고, 탄창을 무한으로 설정했습니다!")
        
end)
