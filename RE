local LocalPlayer = Players.LocalPlayer

local teleportingStatic = false  -- Static cuff ê¸°ëŠ¥ì˜ ìƒíƒœë¥¼ ì¶”ì í•˜ëŠ” ë³€ìˆ˜

local outOfWorldPosition = Vector3.new(98.03588104248047, -17.422523498535156, -61.581932067871094)
local cuffRange = 50  -- ìˆ˜ê°‘ ì±„ìš¸ ìˆ˜ ìˆëŠ” ë²”ìœ„
